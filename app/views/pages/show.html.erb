<div class="row">
  <div class="medium-12 columns">
    <ul class="small-block-grid-1">
      <h1><%= @page.title %></h1>
      <li><%= image_tag(@page.image, id: "comic-panel") %></li>
      <small><%= @page.caption %></small>
    </ul>
    <div class="small-block-grid-1">
      <%= render 'navigation' %>
    </div>
  </div>
</div>
<div>Back to <%= link_to("#{@page.book.title}",@page.book) %></div>
